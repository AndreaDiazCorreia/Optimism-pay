# Private key for signing transactions.
# Note: Do NOT share this private key. Use an account with minimal funds for development and testing purposes.
PRIVATE_KEY=your_private_key_here

# RPC URL for Optimism Goerli network.
OPTIMISM_GOERLI_URL=https://goerli.optimism.io

# RPC URL for Sepolia network.
# You can get a personalized URL using services like Infura or Alchemy.
SEPOLIA_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# RPC URL for Base Goerli network.
BASE_GOERLI_URL=https://goerli.base.org

# API key for Etherscan (or similar services) to automatically verify contracts.
# Note: You need to create an account on Etherscan to obtain this key.
ETHERSCAN_API_KEY=your_etherscan_api_key
